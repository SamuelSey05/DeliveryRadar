{
  "name": "@what3words/javascript-components",
  "version": "4.9.4",
  "description": "what3words javascript sdk and web components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/what3words/what3words.esm.js",
  "types": "dist/types/components.d.ts",
  "files": [
    "dist/",
    "loader/",
    "hydrate/"
  ],
  "private": false,
  "engines": {
    "node": ">=v14.5"
  },
  "pre-commit": {
    "run": [
      "clean",
      "build"
    ],
    "silent": true
  },
  "scripts": {
    "clean": "rm -rf s3 dist loader www",
    "build": "stencil build --docs && webpack",
    "precompile": "scripts/phrase",
    "compile": "npm run clean && npm run build",
    "coverage": "npm run test -- --coverage --reporter=lcov",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test:spec": "stencil test --spec",
    "test:spec:watch": "stencil test --spec --watchAll",
    "test:e2e": "stencil test --e2e",
    "test:e2e:watch": "stencil test --e2e --devtools --watchAll",
    "test:mocha": "mocha --require ts-node/register --require test/hooks.ts 'test/**/*.spec.*' --exit",
    "generate": "stencil generate",
    "puppeteer:install": "unset PUPPETEER_SKIP_CHROMIUM_DOWNLOAD && node ./node_modules/puppeteer/install.js"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.12.11",
    "@stencil/core": "^3.0.0",
    "@what3words/api": "^5.4.0",
    "cross-fetch": "^3.1.0",
    "querystring": "^0.2.1",
    "uuid": "^8.3.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@stencil/angular-output-target": "^0.5.0",
    "@stencil/react-output-target": "^0.4.0",
    "@stencil/sass": "^2.0.3",
    "@stencil/vue-output-target": "^0.7.0",
    "@types/chance": "^1.1.0",
    "@types/googlemaps": "^3.43.3",
    "@types/jest": "^26.0.16",
    "@types/jquery": "^3.5.5",
    "@types/jsdom": "^16.2.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.14.10",
    "@types/qs": "^6.9.5",
    "@types/should": "^13.0.0",
    "@types/sinon": "^10.0.6",
    "@types/uuid": "^8.3.0",
    "babel-loader": "^8.2.2",
    "chance": "^1.1.7",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jsdom": "^19.0.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "puppeteer": "^13.0.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "should": "^13.2.3",
    "sinon": "^12.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.9.5",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  }
}
